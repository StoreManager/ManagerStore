<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    android:gravity="center">

    <CheckBox
        android:id="@+id/cb_gride_item_project_manage_classify_dishes"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="@drawable/select_project_manage_checkbox_style"
        android:textColor="#000"
        android:gravity="center"
        android:button="@null"/>
    <CheckBox
        android:id="@+id/cb_gride_selected_item_project_manage_classify_dishes"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:background="@drawable/bt_gride_classify_selected"
        android:textColor="#000"
        android:gravity="center"
        android:button="@null"
        android:visibility="gone"/>


</LinearLayout>